Table Users {
    id integer [note: 'same endpoints', ref: > id]
    country_id integer [note: 'circular refs', ref: > Country.id] 
}

Ref {
    Users.country_id > Country.id
    Users.id > Users.id
}

Table Country {
    id integer [note: 'circular refs', ref: > Country.id]
 
}

Ref {
    Country.id > Users.country_id
}

Ref {
    Users.country_id > Country.id
}